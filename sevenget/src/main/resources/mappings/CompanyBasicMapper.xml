<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CompanyBasicDAO">

	<select id="getOneTest" parameterType="int" resultType="keyword">
		SELECT *
		FROM test
		WHERE id = #{id}
	</select>

	<select id="getAllCompany" resultType="companybasic">
		SELECT * FROM companybasicinfo
	</select>

	<insert id="insertKeyword">
		INSERT INTO keywords VALUES(#{keyword},#{searchedNum})
	</insert>
</mapper>
